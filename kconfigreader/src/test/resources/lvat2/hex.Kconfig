config DEP_A
    hex "Hex A"
    default 0x0001

config DEP_B
    hex "Hex B"
    default 0x001


config DEP_X1
    bool
    default y if DEP_A="0x0001"

config DEP_X2
    bool
    default y if DEP_A=0x0001

config DEP_X3
    bool
    default y if DEP_B="0x001"

config DEP_X4
    bool
    default y if DEP_B="0x0010"

